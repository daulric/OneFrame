{
  "name": "OneFrame",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "$ignoreUnknownInstances": true,
      "$path": "src/Shared",
      "Packages" :{
        "$path" : "Packages"
      }
    },

    "ServerScriptService": {
      "$className": "ServerScriptService",
      "$ignoreUnknownInstances": true,
      "$path": "src/Server"
    },

    "ReplicatedFirst": {
      "$className": "ReplicatedFirst",
      "$ignoreUnknownInstances": true,
      "$path": "src/Client"
    },

    "StarterPack" : {
      "$className": "StarterPack",
      "$ignoreUnknownInstances": true,
      "$path": "src/Backpack"
    }
  }
}